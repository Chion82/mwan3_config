
config rule 'uestc_lan_1'
	option dest_ip '10.0.0.0/8'
	option proto 'all'
	option sticky '0'
	option use_policy 'gouestc'

config rule 'edu'
	option proto 'all'
	option sticky '0'
	option ipset 'edu_list'
	option use_policy 'gouestc'

config rule 'hosts_sgp'
	option dest_ip '188.166.190.192'
	option proto 'all'
	option sticky '0'
	option use_policy 'gouestc'

config rule 'ddns_ip_check'
	option dest_ip '216.146.43.70'
	option proto 'all'
	option sticky '0'
	option use_policy 'gouestc'

config rule 'https'
	option sticky '1'
	option dest_port '443'
	option proto 'tcp'
	option use_policy 'balanced'

config rule 'default_rule'
	option dest_ip '0.0.0.0/0'
	option use_policy 'balanced'

config interface 'WAN1'
	option enabled '1'
	option reliability '1'
	option count '1'
	option timeout '2'
	option interval '5'
	option down '3'
	option up '3'

config interface 'WAN2'
	option enabled '1'
	option reliability '1'
	option count '1'
	option timeout '2'
	option interval '5'
	option down '3'
	option up '3'

config policy 'balanced'
	option last_resort 'default'
	list use_member 'wan_telecom'
	list use_member 'wan_uestc'

config member 'wan_uestc'
	option interface 'WAN1'
	option weight '1'
	option metric '2'

config member 'wan_telecom'
	option interface 'WAN2'
	option metric '1'
	option weight '1'

config policy 'gouestc'
	list use_member 'wan_uestc'
	option last_resort 'default'

